{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "rewrites": [
    {
      "source": "/(css|js|img|vendor|styles|bootstrap-3\\.3\\.7-dist|screenshot)/(.*)",
      "destination": "/$1/$2"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.php"
    }
  ]
}
